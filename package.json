{
  "name": "sui-dlmm-sdk",
  "version": "1.0.0",
  "description": "SDK for Sui DLMM Protocol with ZK Login support",
  "type": "module",
  "main": "dist/index.cjs.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "types": "./dist/index.d.ts"
    },
    "./managers/*": {
      "import": "./dist/managers/*.js",
      "types": "./dist/managers/*.d.ts"
    },
    "./types/*": {
      "types": "./dist/types/*.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rollup -c",
    "build:dev": "NODE_ENV=development rollup -c",
    "dev": "rollup -c -w", 
    "test": "node --import tsx/esm ./node_modules/.bin/jest",
    "test:watch": "node --import tsx/esm ./node_modules/.bin/jest --watch",
    "test:coverage": "node --import tsx/esm ./node_modules/.bin/jest --coverage",
    "test:testnet": "node --import tsx/esm ./node_modules/.bin/jest --testPathPattern=testnet",
    "test:integration": "node --import tsx/esm ./node_modules/.bin/jest --testPathPattern=integration",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "clean": "rm -rf dist coverage",
    "prepare": "npm run clean && npm run build",
    "prepublishOnly": "npm run type-check && npm run test && npm run build"
  },
  "keywords": [
    "sui",
    "dlmm",
    "defi",
    "amm",
    "blockchain",
    "liquidity",
    "trading",
    "web3",
    "typescript"
  ],
  "author": "Sui DLMM Team",
  "license": "MIT",
  "dependencies": {
    "@mysten/enoki": "^0.3.0",
    "@mysten/sui": ">=1.0.0",
    "@types/node": "^24.2.1",
    "tslib": "^2.6.2",
    "typescript": "^5.9.2"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-typescript": "^12.1.4",
    "@types/jest": "^30.0.0",
    "dotenv": "^16.6.1",
    "jest": "^30.0.5",
    "rollup": "^4.46.2",
    "rollup-plugin-dts": "^6.2.1",
    "ts-jest": "^29.4.1",
    "tsx": "^4.20.3"
  },
  "peerDependencies": {
    "@mysten/sui": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/your-org/sui-dlmm-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/your-org/sui-dlmm-sdk/issues"
  },
  "homepage": "https://github.com/your-org/sui-dlmm-sdk#readme"
}
