# Sui DLMM SDK Testing Configuration
# Copy this file to .env and fill in your values

# Sui Network Configuration
SUI_NETWORK=testnet
SUI_RPC_URL=https://fullnode.testnet.sui.io:443

# Your deployed contract addresses (from your testnet deployment)
DLMM_PACKAGE_ID=0x6a01a88c704d76ef8b0d4db811dff4dd13104a35e7a125131fa35949d0bc2ada
DLMM_FACTORY_ID=0x160e34d10029993bccf6853bb5a5140bcac1794b7c2faccc060fb3d5b7167d7f
TEST_USDC_TREASURY=0x2270d37729375d0b1446c101303f65a24677ae826ed3a39a4bb9c744f77537e9

# Test wallet configuration (for integration tests)
# Generate a test keypair: sui keytool generate ed25519
TEST_PRIVATE_KEY=your_test_private_key_here
TEST_ADDRESS=your_test_address_here

# Test configuration
TEST_TIMEOUT=30000
ENABLE_INTEGRATION_TESTS=true
ENABLE_TESTNET_TESTS=true

# Gas configuration
DEFAULT_GAS_BUDGET=10000000
MAX_GAS_BUDGET=50000000